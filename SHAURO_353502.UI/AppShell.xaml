<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SHAURO_353502.UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SHAURO_353502.UI"
    xmlns:pages="clr-namespace:SHAURO_353502.UI.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="Hotel Room Service">

    <ShellContent
        Title="Home"
        Icon="home.png"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Hotel rooms"
        Icon="planet.png"
        ContentTemplate="{DataTemplate pages:HotelRoomsPage}"
        Route="HotelRoomsPage" />

</Shell>
